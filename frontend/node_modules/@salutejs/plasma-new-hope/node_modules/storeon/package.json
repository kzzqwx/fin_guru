{
  "name": "storeon",
  "version": "3.1.5",
  "description": "Tiny (180 bytes) event-based Redux-like state manager for React and Preact",
  "keywords": [
    "state",
    "immutable",
    "react",
    "preact",
    "hook"
  ],
  "author": "Andrey Sitnik <andrey@sitnik.ru>",
  "license": "MIT",
  "repository": "storeon/storeon",
  "sideEffects": false,
  "types": "./index.d.ts",
  "peerDependencies": {
    "preact": ">=10.0.0",
    "react": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "preact": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "type": "module",
  "main": "index.cjs",
  "module": "index.js",
  "react-native": "index.js",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "require": "./index.cjs",
      "import": "./index.js",
      "default": "./index.js"
    },
    "./index.d.ts": "./index.d.ts",
    "./package.json": "./package.json",
    "./devtools/package.json": "./devtools/package.json",
    "./devtools": {
      "browser": {
        "development": "./devtools/index.dev.js",
        "production": "./devtools/index.prod.js",
        "default": "./devtools/index.prod.js"
      },
      "require": "./devtools/index.cjs",
      "import": "./devtools/index.js",
      "default": "./devtools/index.js"
    },
    "./preact/package.json": "./preact/package.json",
    "./preact": {
      "browser": {
        "development": "./preact/index.dev.js",
        "production": "./preact/index.prod.js",
        "default": "./preact/index.prod.js"
      },
      "require": "./preact/index.cjs",
      "import": "./preact/index.js",
      "default": "./preact/index.js"
    },
    "./react/package.json": "./react/package.json",
    "./react": {
      "browser": {
        "development": "./react/index.dev.js",
        "production": "./react/index.prod.js",
        "default": "./react/index.prod.js"
      },
      "require": "./react/index.cjs",
      "import": "./react/index.js",
      "default": "./react/index.js"
    }
  }
}